<%= form_with(url: tasks_path, local: true, method: :get) do |form| %>
  <%= form.text_field :search, placeholder: "タイトル" %>
  <%= form.select :status, Task.statuses.keys, prompt: "ステータス" %>
  <%= form.select :label_ids, Label.pluck(:label_title, :id), prompt: "ラベル" %>
  <%= form.submit '検索', class: 'btn btn-outline-dark btn-sm' %>
<% end %>
